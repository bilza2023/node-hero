<head>
  <link rel="stylesheet" href="/css/water.css">
</head>

<h1>Edit Tcode</h1>

<%- include('../../partials/flash.ejs') %>  

<form method="POST" action="/admin/tcode/<%= tcode.id %>">
  <p><strong>Tcode Name (locked):</strong> <%= tcode.tcodeName %></p>

  <label>
    Title:
    <input type="text" name="title" value="<%= tcode.title %>" required>
  </label>

  <button type="submit">Save</button>
</form>

<a href="/admin/tcode">Back</a>
